{
    "name": "phpmetrics/symfony-bundle",
    "replace": {
        "halleck45/phpmetrics-collector-bundle": "*"
    },
    "description": "Display PhpMetrics informations in Symfony Debug bar",
    "keywords": ["symfony", "symfony4", "symfony5", "phpmetrics"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-François Lépine",
            "homepage": "http://blog.lepine.pro"
        },
        {
            "name": "Eric COURTIAL"
        }
    ],
    "require": {
        "phpmetrics/phpmetrics": "^2.5",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/yaml": "^4|^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.14",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^3.0|^4.0",
        "symfony/class-loader": "^3.0",
        "symfony/finder": "^3.0|^4.0",
        "symfony/form": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^5.0",
        "symfony/templating": "^3.0|^4.0",
        "symfony/twig-bundle": "^3.0|^4.0",
        "symfony/web-profiler-bundle": "^3.0|^4.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Hal\\Bundle\\PhpMetricsCollector\\": "src/Hal/Bundle/PhpMetricsCollector/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\": "testing/application/src/My/"
        },
        "classmap": [
            "testing/application/AppKernel.php"
        ]
    }
}
