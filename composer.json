{
    "name": "halleck45/phpmetrics-collector-bundle",
    "description": "Display PhpMetrics informations in Symfony2 Debug bar",
    "keywords": ["symfony2", "phpmetrics"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-François Lépine",
            "homepage": "http://blog.lepine.pro"
        }
    ],
    "require": {
        "symfony/http-kernel": "^2.2|^3.0|^4.0",
        "phpmetrics/phpmetrics": "^2.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "^2.2|^3.0|^4.0",
        "squizlabs/php_codesniffer": "^3.1",
        "phpunit/phpunit": "^4.8.27",
        "symfony/twig-bundle": "^2.2|^3.0|^4.0",
        "symfony/web-profiler-bundle": "^2.2|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Hal\\Bundle\\PhpMetricsCollector\\": "src/Hal/Bundle/PhpMetricsCollector/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\": "testing/application/src"
        }
    }
}
